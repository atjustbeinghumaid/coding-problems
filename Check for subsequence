typedef long long ll;
using namespace std;
int main()
 {
	//code
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
	string A,B;
	cin>>A>>B;
	int idx=0;
	for(int i=0;i<B.size();i++){
	    if(B[i]==A[idx])
	        idx++;
	}
	if(idx==A.length())
	cout<<1<<endl;
	else
	cout<<0<<endl;
	}
	return 0;
}
