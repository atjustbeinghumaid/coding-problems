#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
ll t,p1,p2,k;
cin>>t;
    while(t--)
    {
    cin>>p1>>p2>>k;
        if(p1+p2==0||p1+p2<k)
        cout<<"CHEF\n";
        else if((p1+p2)%(2*k)>=0&&(p1+p2)%(2*k)<k)
        cout<<"CHEF\n";
        else
        cout<<"COOK\n";
    }

}
