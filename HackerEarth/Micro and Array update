#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long t,i,n,k,c=0,min=INT_MAX;
    
    cin>>t;
    while(t--)
    {
        min=INT_MAX;
        cin>>n>>k;
        int a[n];
        for(i=0;i<n;i++)
        cin>>a[i];
        for(i=0;i<n;i++)
        {
            if(a[i]<min)
            min=a[i];
        }
        c=abs(min-k);
        cout<<c<<endl;
    }
}
