#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
 
    ll i,n,q,x,l,h;
    short type;
    cin>>n;
    char s[n];
    for(i=1;i<=n;i++)
        cin>>s[i];
    cin>>q;
    while(q--)
    {
        cin>>type;
        if(type==1)
        {
            cin>>x;
            if(s[x]==0)
                s[x]=1;
            else
                s[x]=0;
        }
        else
            {
            cin>>l>>h;
            for(i=l;i<=h;i++)
                {
                    if(s[i]=='0')
                        {
                            cout<<i<<endl;
                            break;
                        }
                        if(i==h)
                            cout<<-1<<endl;
 
                }
 
    }
}
}
