#include <iostream>
using namespace std;
int main()
{
    int c=0,c1=0,i;
    long N,X;
    cin>>N>>X;
    long a[N];
    for( i=0;i<N;i++)
    cin>>a[i];
    for(i=0;i<N;i++)
    {
        if(a[i]<=X)
        ++c;
        else
        c1++;
        if(c1>1)
        break;    
    }
    cout<<c;
    
    return 0;
}
