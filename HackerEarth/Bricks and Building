#include <iostream>
using namespace std;

int main()
{
    int n,i,c,k;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    cin>>a[i];
    int q;
    cin>>q;
    while(q--)
    {
        c=0;
        cin>>k;
        for(i=0;i<n;i++)
        if(a[i]%k==0)
        c++;
        cout<<c<<"\n";
    }
    return 0;
}
