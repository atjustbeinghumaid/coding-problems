#include<bits/stdc++.h>
using namespace std;
typedef long long ll;
int main()
{
    ll t,n,x1,y1,x2,y2,a,b,c=0;
    cin>>t;
    while(t--)
    {
        c=0;
        cin>>x1>>y1>>x2>>y2;
        cin>>n;
        while(n--)
        {
            cin>>a>>b;
            if((a>=x1||a<=x2)&&(b>=y1||b<=y2))
            c++;
        }
        cout<<c<<endl;
        
}
}
