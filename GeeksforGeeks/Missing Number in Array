#include<bits/stdc++.h>
typedef long long ll;
using namespace std;
int main()
 {
	//code
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin>>t;
	while(t--){
	int n;
	cin>>n;
	int a=0,b=0,x;
	for(int i=1;i<=n;i++)
	a = a ^ i;
	for(int i=0;i<n-1;i++){
	    cin>>x;
	    b = b ^ x;
	}
	int result = a^b;
    cout<<result<<endl;	
	}
	return 0;
}
