using namespace std;
typedef long long ll;
int main()
 {
	//code
	int t;
	cin>>t;
	while(t--){
	    ll n,cnt=0;
	    cin>>n;
	    vector<ll>a(n);
	    for(ll i=0;i<n;i++)
	    cin>>a[i];
	    for(ll i=0;i<n;i++){
	        cnt=0;
	        for(ll j=i+1;j<n;j++){
	            if(a[j]<a[i])
	            cnt++;
	        }
	        cout<<cnt<<" ";
	        
	    }
	    cout<<"\n";
	}
	return 0;
}
